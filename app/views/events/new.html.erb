<%= form_with model: @event, url: user_events_path(current_user), local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :date %>
    <%= form.date_select :date %>
  </div>

  <div>
    <%= form.label :venue %>
    <%= form.text_field :venue %>
  </div>

  <div>
    <%= form.submit "Create Event" %>
  </div>
<% end %>

<%= link_to "Back", root_path %>
